typedef	struct {
        char    hg_or_hb_code[1];   /*  hg=1, hb=2  */
	char	slic_refnum[32];
	char	slic_cage[5];
	char	eol;
	} SLIC_EXTRCT_RCD;

typedef	struct {
        char    hg_or_hb_code[1];   /*  hg=1, hb=2  */
	char	slic_refnum_cmprsd[32];
	char	slic_refnum[32];
	char	slic_cage[5];
	char	eol;
	} SLIC_EXTRCT_CMPRSD_RCD;
