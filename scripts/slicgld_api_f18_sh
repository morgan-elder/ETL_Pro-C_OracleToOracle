  echo -e "#*******************************************************************#" 
  echo "#     Extract HG Change Activity - "`date` 
  echo "#*******************************************************************#" 
  echo "#" 
  export SLICGLD_HG=$DATA_DIR/$PROCESSING_MODEL"_api_slicgld_hg.dta"
