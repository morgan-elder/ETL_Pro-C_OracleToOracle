    rc=0
  else
    echo -e "##############################################################" 
    echo "# Unsuccessful completion of SLIC/Gold HG Change Activity Extract Program - `date`" 
    echo $scm_exe/slicgldHG -a$SLICGLD_HG -b$PROCESSING_MODEL
    echo "##############################################################" 
